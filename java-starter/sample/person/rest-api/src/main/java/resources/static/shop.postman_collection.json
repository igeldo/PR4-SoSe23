		{
			"name": "Login",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var data = JSON.parse(responseBody);",
							"postman.clearGlobalVariable(\"access_token\");",
							"postman.setGlobalVariable(\"access_token\", data.access_token);"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "urlencoded",
					"urlencoded": [
						{
							"key": "client_id",
							"value": "java-starter-client",
							"type": "text"
						},
						{
							"key": "username",
							"value": "java-starter-user",
							"type": "text"
						},
						{
							"key": "password",
							"value": "passKeycloakUser",
							"type": "text"
						},
						{
							"key": "grant_type",
							"value": "password",
							"type": "text"
						}
					]
				},
				"url": {
					"raw": "{{host.idm}}:{{port.idm}}/auth/realms/java-starter-realm/protocol/openid-connect/token",
					"host": [
						"{{host.idm}}"
					],
					"port": "{{port.idm}}",
					"path": [
						"auth",
						"realms",
						"java-starter-realm",
