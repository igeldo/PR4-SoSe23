				"disabledSystemHeaders": {
					"accept": true
				}
			},
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Accept",
						"value": "application/json",
						"type": "text"
					},
					{
						"key": "Authorization",
						"value": "Bearer {{access_token}}",
						"type": "default"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"vorname\": \"{{$randomFirstName}}\",\n    \"name\": \"{{$randomLastName}}\"\n}",
					"options": {
						"raw": {
							"language": "json"
